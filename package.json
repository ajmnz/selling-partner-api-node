{
  "name": "sp-api-node",
  "version": "3.0.0",
  "main": "./index.js",
  "types": "./index.d.ts",
  "author": "ajmnz <arnau@ajb.cat>",
  "license": "MIT",
  "description": "ðŸ›¡ Fully typesafe Amazon Selling Partner API SDK for Node.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ajmnz/selling-partner-api-node"
  },
  "keywords": [
    "sp api",
    "amazon sp api",
    "amazon",
    "selling partner api",
    "selling partner",
    "selling partner api sdk",
    "node.js",
    "sdk",
    "typescript"
  ],
  "packageManager": "yarn@3.3.1",
  "scripts": {
    "build": "tsc && yarn copy:packageJson",
    "copy:packageJson": "copyfiles package.json README.md dist",
    "lint": "eslint --fix src",
    "format": "prettier --write src",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "clean": "rimraf -rf dist",
    "release": "dotenv -- release-it"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "copyfiles": "2.4.1",
    "dotenv-cli": "7.4.2",
    "eslint": "8.57.1",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-tsdoc": "0.3.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.3.0",
    "pinst": ">=2",
    "prettier": "2.8.8",
    "release-it": "15.11.0",
    "rimraf": "4.4.1",
    "swagger-typescript-api": "12.0.4",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "aws4-axios": "2.4.9",
    "axios": "0.27.2",
    "form-data": "4.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,css,less,ejs}": [
      "prettier --write"
    ],
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write",
    "*.src": "prettier --write"
  }
}
